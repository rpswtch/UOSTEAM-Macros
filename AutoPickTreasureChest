@pushlist 'Trash' 0x1452 //Butcher Knife
useobject 'Chest'
pause 900
while diffhits >= 20
  if poisoned
    cast 'cure'
    autotargetself
  endif
  bigheal 'self'
  pause 4000
endwhile
for 0 to Loot
  if @inrange 'Chest' 2
    if @movetype Loot[] 'Chest' 'Lootbag'
      pause 1000
    endif
  endif
endfor
for 0 to Trash
  if @inrange 'Chest' 2
    if @movetype Trash[] 'Chest' 'TrashCan'
      pause 1000
    endif
  endif
endfor
